# Main Environment Configuration File
# PostgreSQL Database
POSTGRES_DB=postgres_ai
POSTGRES_USER=postgres_admin
POSTGRES_PASSWORD=secure_postgres_password_2025

# pgAdmin
PGADMIN_EMAIL=admin@postgresai.local
PGADMIN_PASSWORD=pgadmin_secure_password_2025

# OpenWebUI
WEBUI_SECRET_KEY=openwebui_secret_key_very_secure_2025
WEBUI_JWT_SECRET_KEY=jwt_secret_key_very_secure_2025

# Tailscale
TS_AUTHKEY=your_tailscale_auth_key_here
TS_HOSTNAME=postgres-ai-stack

# SSL Configuration
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# TinyBERT
FLASK_ENV=production
MODEL_NAME=distilbert-base-uncased

# Network Configuration
NETWORK_SUBNET=172.20.0.0/16
EXTERNAL_DOMAIN=your-domain.com

# Security
JWT_SECRET=very_secure_jwt_secret_2025
API_KEY=global_api_key_2025
